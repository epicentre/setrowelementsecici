{
  "name": "setrowelementsecici",
  "version": "1.0.0",
  "description": "Websitenizdeki html elementlerin css selector yolunu bulmanızı sağlar.",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy": "rm -rf ./bin && mkdir -p ./bin && cp src/manifest.json bin/manifest.json && cp src/background.js bin/background.js && cp src/*.png bin/",
    "build": "npm run copy && webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epicentre/setrowelementsecici"
  },
  "keywords": [
    "get",
    "css",
    "selector",
    "find",
    "unique",
    "browser",
    "extension"
  ],
  "author": "AntonLapshin",
  "license": "MIT",
  "homepage": "https://github.com/epicentre/setrowelementsecici#readme",
  "devDependencies": {
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@medv/finder": "^1.1.2",
    "lodash": "^4.17.5"
  }
}
